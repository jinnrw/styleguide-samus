        // ===== 3.3 Section - Pagination =====
        section.button-content.pagination
              
              .grid-25.grid-sm-100
                h3 Pagination
                p This element is used in the product page to navigate through the preview images of a product.

              .grid-70.grid-sm-100
                //- 3.3.1
                .row.pagination-container
                  .pagination-links
                    a.page-arrow.page-previous.hidden
                    .page-num.current-page
                      a 1
                    .page-num
                      a 2
                    .page-num
                      a 3
                    .page-num
                      a 4
                    .page-num
                      a 5
                    .page-num
                      a ...
                    .page-num
                      a 17
                    a.page-arrow.page-next
                  p.pagination-desc First Page Selected
                //- 3.3.2
                .row.pagination-container
                  .pagination-links
                    a.page-arrow.page-previous
                    .page-num
                      a 1
                    .page-num
                      a 2
                    .page-num.current-page
                      a 3
                    .page-num
                      a 4
                    .page-num
                      a 5
                    .page-num
                      a ...
                    .page-num
                      a 17
                    a.page-arrow.page-next
                  p.pagination-desc Page Selected in the Middle
                //- 3.3.3
                .row.pagination-container
                  .pagination-links
                    a.page-arrow.page-previous
                    .page-num
                      a 1
                    .page-num
                      a ...
                    .page-num
                      a 13
                    .page-num
                      a 14
                    .page-num
                      a 15
                    .page-num
                      a 16
                    .page-num.current-page
                      a 17
                    a.page-arrow.page-next.hidden
                  p.pagination-desc Last Page Selected
                //- 3.3.4
                .row.pagination-container
                  .pagination-links
                    a.page-arrow.page-previous
                    .page-num
                      a 1
                    .page-num.current-page
                      a 2
                    .page-num
                      a 3
                    .page-num
                      a 4
                    .page-num.page-hover
                      a 5
                    .page-num
                      a ...
                    .page-num
                      a 17
                    a.page-arrow.page-next
                  p.pagination-desc Page on Hover

