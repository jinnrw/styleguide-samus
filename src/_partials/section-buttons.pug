#section-buttons.buttons.main
      
  .content

    // ===== Sections =====
    .section.sec-buttons
      article.section-article.buttons-article
              

        // =====Sections--CTA=====
        section.animated.delay
          .section-inner
            h1.section-inner__title.show-text Controls.
        section.button-content.button-stat#controls-primary-btns
          .row
            h3 Primary CTA
            .grid-25.grid-sm-100.button-description
              p The primary green CTA is used to indicate continuation to a subsequent page.
              h6 Specifications
              ul
                li Height: 40px;
                li Padding: 20px;
                li Font Size: 16px;
                li Font Weight: 700;
                li Stroke: 1px;

            .grid-70.grid-sm-100.button-container
              .button-grid-layout
                .button--container
                  //- Button component here
                  include components/button_comp
                    block button-title
                      h1.button--title Button 
                  
                    block button-annotation
                      .button__annotation-top-text 20px
                      .button__annotation-top-line
                      .button__annotation-left-text 40px
                      .button__annotation-left-line
                      .button__annotation-top-right-circle
                      .button__annotation-bottom-right-circle
                      .button__annotation-right-text 2px Radius Corners
                      

            //- different button states
          .row.button-states
            .grid-25.grid-sm-100.button-description
              p Length of CTA Varies on product title, please maintain padding proportions as displayed.

            .grid-70.grid-sm-100.button-container.focus-button-container

              .button-grid-layout
                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Primary 
                    block button-desc-container
                      .button_desc_container
                        .button__desc Fill: #4CA90C

                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Hover 
                    block button-desc-container
                      .button_desc_container
                        .button__desc Fill: #8CC764

                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Disabled
                    block button-desc-container
                      .button_desc_container
                        .button__desc Border/Fill: #E6F3DD

                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Primary
                    block button-desc-container
                      .button_desc_container
                        .button__desc Fill: #FFFFFF
                        .button__desc Text: #4CA90C
                        .button__desc Border: #4CA90C

                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Hover
                    block button-desc-container
                      .button_desc_container
                        .button__desc Fill: #B2CFE5
                        .button__desc Text: #4CA90C
                        .button__desc Border: #4CA90C

                .button--container
                  include components/button_comp
                    block button-title
                      h1.button--title Disabled
                    block button-desc-container
                      .button_desc_container
                        .button__desc Fill: #B2CFE5
                        .button__desc Text: #D9ECCC
                        .button__desc Border: #D9ECCC

